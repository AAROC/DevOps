[sl5-base]
name=SL 5 base
baseurl=http://ftp.scientificlinux.org/linux/scientific/510/$basearch/SL
                http://ftp1.scientificlinux.org/linux/scientific/510/$basearch/SL
                http://ftp2.scientificlinux.org/linux/scientific/510/$basearch/SL
                ftp://ftp.scientificlinux.org/linux/scientific/510/$basearch/SL
#mirrorlist=ftp://ftp.scientificlinux.org/linux/scientific/mirrorlist/sl-base-510.txt
enabled=1
gpgcheck=1
# To use priorities you must have yum-priorities installed
priority=2
exclude=perl-DBI mysql51*
protect=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-sl file:///etc/pki/rpm-gpg/RPM-GPG-KEY-sl5 file:///etc/pki/rpm-gpg/RPM-GPG-KEY-csieh file:///etc/pki/rpm-gpg/RPM-GPG-KEY-dawson file:///etc/pki/rpm-gpg/RPM-GPG-KEY-jpolok file:///etc/pki/rpm-gpg/RPM-GPG-KEY-cern file:///etc/pki/rpm-gpg/RPM-GPG-KEY-CentOS-5
